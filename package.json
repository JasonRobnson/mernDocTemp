{
	"name": "bosh",
	"version": "1.0.0",
	"description": "Helping out the our sistahs.",
	"main": "server.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"client-install": "npm install --prefix stockclient",
		"start": "concurrently \"npm run server\" \"npm run stockclient\"",
